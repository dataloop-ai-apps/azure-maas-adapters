{
  "name": "azure-api-cohere-embed-v3-multilingual",
  "displayName": "Cohere-embed-v3-multilingual",
  "version": "0.0.10",
  "scope": "public",
  "description": "Cohere-embed-v3-multilingual from Azure API",
  "attributes": {
    "Provider": "Cohere",
    "Deployed By": "Microsoft",
    "Category": "Model",
    "NLP": "Embeddings",
    "Media Type": [
      "Text"
    ]
  },
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/azure-maas-adapters.git",
    "gitTag": "0.0.10"
  },
  "components": {
    "computeConfigs": [
      {
        "name": "cohere-embed-v3-multilingual-azure-deploy",
        "runtime": {
          "podType": "regular-xs",
          "concurrency": 10,
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/cpu/openai:1.0.0",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2
          }
        }
      }
    ],
    "modules": [
      {
        "name": "cohere-embed-v3-multilingual-azure-module",
        "entryPoint": "models/base_embedder.py",
        "className": "ModelAdapter",
        "computeConfig": "cohere-embed-v3-multilingual-azure-deploy",
        "description": "Cohere-embed-v3-multilingual Azure API Adapter",
        "integrations": [
          "dl-azure-api-key"
        ],
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          }
        ],
        "functions": [
          {
            "name": "embed_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The input items for embeddings."
              }
            ],
            "output": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The same input items for embeddings."
              },
              {
                "type": "Json",
                "name": "json",
                "description": "Embeddings of items."
              }
            ],
            "displayName": "Embed Items",
            "displayIcon": "",
            "description": "The inference function of the model."
          },
          {
            "name": "embed_dataset",
            "input": [
              {
                "type": "Dataset",
                "name": "dataset",
                "description": "The input dataset of the items required for prediction."
              },
              {
                "type": "Json",
                "name": "filters",
                "description": "The DQL in json format to get all the items required for prediction."
              }
            ],
            "output": [],
            "displayName": "Embed Dataset",
            "displayIcon": "",
            "description": "Inference function of the model on a dataset."
          }
        ]
      }
    ],
    "models": [
      {
        "name": "cohere-embed-v3-multilingual-azure",
        "moduleName": "cohere-embed-v3-multilingual-azure-module",
        "scope": "project",
        "status": "pre-trained",
        "configuration": {
          "endpoint-url": "",
          "embeddings_size": 1024
        },
        "description": "The Cohere-embed-v3-multilingual model on Azure API delivers advanced embedding capabilities for efficient natural language processing across multiple languages."
      }
    ]
  }
}